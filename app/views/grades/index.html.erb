<%= render 'layouts/flash_messages' %>
<div class="center jumbotron">
    <h1>Welcome to the Sample App</h1>

    <h2>
        This is the home page for the
        <a href="heeps://railstutorial.jp/">Ruby on Rails Tutorial</a>>
        sample application.
    </h2>

    <% @grades.each do |grade|  %>
    <div class="container container-m">
        <div class="row">
            <p class="mb-0" >name | <%= user_name_is? grade %></p>
            <p class="mb-0" >A+ | <%= grade.AA %></p>
            <p class="mb-0" >A  | <%= grade.A %></p>
            <p class="mb-0" >B  | <%= grade.B %></p>
            <p class="mb-0" >C  | <%= grade.C %></p>
            <p class="mb-0" >D  | <%= grade.D %></p>
            <p class="mb-0" >E  | <%= grade.E %></p>
            <p class="mb-0" >F  | <%= grade.F %></p>
        </div>
    </div>
    <% end %>
</div>

